{
  "name": "mononome",
  "homepage": "http://mononome.com/v2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^13.4.0",
    "contentful": "^7.14.3",
    "lodash": "^4.17.15",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-modal-video": "^1.1.2",
    "react-scripts": "1.1.1",
    "react-scrollable-anchor": "^0.6.1"
  },
  "scripts": {
    "start": "npm-run-all -p start:scss start:js",
    "build:scss": "node-sass --include-path ./scss ./scss/index.scss src/index.css",
    "start:scss": "node-sass --include-path ./scss -w ./scss/index.scss src/index.css",
    "start:js": "react-scripts start",
    "build": "npm run build:scss && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2"
  }
}
